// Consider adding the 1918 zones here, if they are not used in your
// organization
//include "/etc/bind/zones.rfc1918";

zone "k8s.zone" IN {
	type master;
	file "/etc/bind/zones/db.fwd.k8s.zone";
	allow-update { none; };
};

zone "33.168.192.in-addr.arpa" IN {
	type master;
	file "/etc/bind/zones/db.rev.k8s.zone";
	allow-update { none; };
};